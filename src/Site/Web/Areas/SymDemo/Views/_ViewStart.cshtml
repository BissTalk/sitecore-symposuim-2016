@{
    // Use "SymDemo" area for all views/partials on pages that use MainLayout.
    // This will enable (Partial)View resolution by area
    if (ViewContext != null && ViewContext.RouteData != null && ViewContext.RouteData.DataTokens != null)
    {
        ViewContext.RouteData.DataTokens["area"] = "SymDemo";
    }
}
